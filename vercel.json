{
  "version": 2,
  "functions": {
    "server.js": {
      "maxDuration": 60,
      "memory": 3008
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true",
    "PUPPETEER_CACHE_DIR": "/tmp/.cache/puppeteer",
    "VERCEL": "1",
    "NODE_OPTIONS": "--max-old-space-size=3008"
  }
}
